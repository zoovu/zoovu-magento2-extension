<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="sitesearch360-script" type="host">*.sitesearch360.com</value>
                <value id="semknox-script" type="host">*.semknox.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="sitesearch360-style" type="host">*.sitesearch360.com</value>
                <value id="semknox-style" type="host">*.semknox.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="sitesearch360-connect" type="host">*.sitesearch360.com</value>
                <value id="semknox-connect" type="host">*.semknox.com</value>
            </values>
        </policy>
        <policy id="media-src">
            <values>
                <value id="sitesearch360-media" type="host">*.sitesearch360.com</value>
                <value id="semknox-media" type="host">*.semknox.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="sitesearch360-img" type="host">*.sitesearch360.com</value>
                <value id="semknox-img" type="host">*.semknox.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>